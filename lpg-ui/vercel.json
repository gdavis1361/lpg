{
  "version": 2,
  "buildCommand": "NODE_OPTIONS='--require ./module-patch.js' SKIP_TYPESCRIPT_CHECK=true next build",
  "ignoreCommand": "echo 'Running TypeScript check...' && exit 0",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_DISABLE_ESLINT": "1",
    "SKIP_TYPESCRIPT_CHECK": "true", 
    "TS_NODE_TRANSPILE_ONLY": "true",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "github": {
    "silent": false,
    "autoJobCancelation": true
  }
}
