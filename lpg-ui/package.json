{
  "name": "lpg-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run types:supabase && next dev --turbopack",
    "build": "npm run vercel-build",
    "prevercel-build": "node prestart.js",
    "vercel-build": "NODE_OPTIONS='--require ./module-patch.js' next build",
    "start": "next start",
    "lint": "next lint",
    "validate-env": "node scripts/validate-env.js",
    "types:supabase": "supabase gen types typescript --project-id tfvnratitcbmqqrsjamp --schema public > src/types/supabase.ts",
    "test": "vitest",
    "diagnose": "node prestart.js && node -e \"require('./debug-config').checkNativeModules()\""
  },
  "dependencies": {
    "@magicuidesign/mcp": "^1.0.6",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/supabase-js": "^2.39.7",
    "lightningcss-wasm": "^1.23.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.5",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jsdom": "^24.1.0",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
